{
    "name": "@script-development/vue-services",
    "version": "0.2.5",
    "description": "Services for Vue SPA App",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "types",
        "src"
    ],
    "scripts": {
        "test": "nyc mocha --recursive --require @babel/register",
        "build": "eslint ./src && rollup src/index.js --file dist/index.js --format iife",
        "dev": "rollup -c scripts/config.js",
        "watch": "rollup -c scripts/config.js --watch",
        "pub": "npm version patch && npm publish",
        "docs": "typedoc",
        "lint": "eslint ./src",
        "mutate": "stryker run"
    },
    "author": "Script",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.1",
        "@babel/preset-env": "^7.11.5",
        "@babel/register": "^7.11.5",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@stryker-mutator/core": "^3.3.1",
        "@stryker-mutator/javascript-mutator": "^3.3.1",
        "@stryker-mutator/mocha-runner": "^3.3.1",
        "axios-mock-adapter": "^1.18.2",
        "eslint": "^7.7.0",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.0",
        "rollup": "^2.26.11",
        "typedoc": "^0.19.1",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "axios": "0.20.0",
        "bootstrap-vue": "^2.15.0",
        "vue": "2.6.12",
        "vue-router": "3.4.3",
        "vuex": "3.5.1"
    },
    "peerDependencies": {
        "@msgpack/msgpack": "^1.12.2",
        "vue-multiselect": "^2.1.6"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|json)\""
        }
    },
    "nyc": {
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "bin": {
        "servvue": "./cli/index.js"
    }
}
