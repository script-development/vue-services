{
    "name": "@script-development/vue-services",
    "version": "1.0.0",
    "description": "Services for Vue SPA App",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "types",
        "src",
        "cli"
    ],
    "repository": "github:script-development/vue-services",
    "scripts": {
        "dev": "vite serve dev",
        "test": "nyc mocha --require @babel/register --recursive",
        "test:watch": "mocha --require @babel/register --watch --recursive",
        "build": "rollup -c scripts/config.js",
        "watch": "rollup -c scripts/config.js --watch",
        "pub": "npm version patch && npm publish",
        "docs": "typedoc",
        "lint": "eslint ./src",
        "mutate": "stryker run",
        "prepare": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "author": "Script",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.11.5",
        "@babel/register": "^7.11.5",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@stryker-mutator/core": "^3.3.1",
        "@stryker-mutator/javascript-mutator": "^3.3.1",
        "@stryker-mutator/mocha-runner": "^3.3.1",
        "@vitejs/plugin-vue": "^1.1.5",
        "@vue/compiler-sfc": "^3.0.2",
        "axios-mock-adapter": "^1.18.2",
        "eslint": "^7.12.1",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-vue": "^7.7.0",
        "faker": "^5.1.0",
        "husky": "^5.1.3",
        "miragejs": "^0.1.41",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.1.0",
        "rollup": "^2.26.11",
        "sinon": "^9.2.1",
        "typedoc": "^0.20.28",
        "typescript": "^4.0.2",
        "vite": "^2.0.4"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "vue": "^3.0.4",
        "vue-router": "^4.0.4"
    },
    "peerDependencies": {
        "@msgpack/msgpack": "^1.12.2"
    },
    "nyc": {
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "bin": {
        "servvue": "./cli/index.js"
    }
}
